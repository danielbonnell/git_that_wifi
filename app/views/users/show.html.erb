<div class="col-md-6 col-md-offset-6">
  <div class="row">
    <div class="panel-heading">
      <ul class="pricing-table">
        <li class="title">My Account</li>
        <li class="price"><%= @user.first_name %> <%= @user.last_name %></li>
        <li class="bullet-item"><%= @user.email %></li>
        <li class="bullet-item"><%= image_tag @user.gravatar_url(:size => 300) %></li>
        <li class="cta-button"><a class="button round" href="<%= edit_user_registration_path %>">Edit My Account</a></li>
      </ul>
    </div>
  </div>
</div>
